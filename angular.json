{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  },
  "version": 1,
  "newProjectRoot": ".",
  "projects": {
    "angularfire-compat-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "angularfire-compat-app",
      "sourceRoot": "angularfire-compat-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angularfire-compat-app",
            "index": "angularfire-compat-app/src/index.html",
            "main": "angularfire-compat-app/src/main.ts",
            "polyfills": "angularfire-compat-app/src/polyfills.ts",
            "tsConfig": "angularfire-compat-app/tsconfig.app.json",
            "assets": [
              "angularfire-compat-app/src/favicon.ico",
              "angularfire-compat-app/src/assets"
            ],
            "styles": ["angularfire-compat-app/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "angularfire-compat-app/src/environments/environment.ts",
                  "with": "angularfire-compat-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angularfire-compat-app:build:production"
            },
            "development": {
              "browserTarget": "angularfire-compat-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angularfire-compat-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "angularfire-compat-app/src/test.ts",
            "polyfills": "angularfire-compat-app/src/polyfills.ts",
            "tsConfig": "angularfire-compat-app/tsconfig.spec.json",
            "karmaConfig": "angularfire-compat-app/karma.conf.js",
            "assets": [
              "angularfire-compat-app/src/favicon.ico",
              "angularfire-compat-app/src/assets"
            ],
            "styles": ["angularfire-compat-app/src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "angularfire-compat-app/**/*.ts",
              "angularfire-compat-app/**/*.html"
            ]
          }
        },
        "cypress-run": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "angularfire-compat-app:serve",
            "configFile": "angularfire-compat-app/cypress.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angularfire-compat-app:serve:production"
            }
          }
        },
        "cypress-open": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "watch": true,
            "headless": false,
            "configFile": "angularfire-compat-app/cypress.json"
          }
        },
        "e2e": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "angularfire-compat-app:serve",
            "watch": true,
            "headless": false
          },
          "configurations": {
            "production": {
              "devServerTarget": "angularfire-compat-app:serve:production"
            }
          }
        }
      }
    },
